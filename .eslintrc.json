{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "no-extra-semi": 0,
        "semi": 0,
        "class-methods-use-this": 0,
        "import/prefer-default-export": 0
    }
}
